{
  "name": "r3web",
  "description": "API and Front end to R3",
  "version": "1.0.0",
  "author": "Titan",
  "private": true,
  "scripts": {
    "docs": "php artisan swagger-lume:generate",
    "webpack:visualise": "webpack --json > stats.json",
    "clean": "npm run clean:assets && npm run clean:cache",
    "clean:assets": "rimraf public/assets/*",
    "clean:cache": "php artisan cache:clear",
    "dev": "npm run clean && npm-run-all --parallel dev:*",
    "dev:js": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules",
    "dev:api": "php artisan serve",
    "build": "npm run clean && npm run build:js",
    "build:js": "cross-env NODE_ENV=production webpack --progress --hide-modules"
  },
  "dependencies": {
    "axios": "^0.15.3",
    "font-awesome": "^4.7.0",
    "leaflet": "^1.0.3",
    "leaflet-rastercoords": "^1.0.2",
    "lodash.find": "^4.6.0",
    "lodash.foreach": "^4.5.0",
    "lodash.map": "^4.6.0",
    "moment": "^2.17.1",
    "vue": "^2.1.0",
    "vue-cookie": "^1.1.3",
    "vue-i18n": "^5.0.3",
    "vue-router": "^2.2.1",
    "vue-search-select": "^2.3.6",
    "vuex": "^2.1.2",
    "vuex-persistedstate": "^1.3.0"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.0.0",
    "babel-loader": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "npm-run-all": "^4.0.2",
    "rimraf": "^2.6.1",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.5.0",
    "url-loader": "^0.5.7",
    "vue-loader": "^10.0.0",
    "vue-template-compiler": "^2.1.0",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  }
}
